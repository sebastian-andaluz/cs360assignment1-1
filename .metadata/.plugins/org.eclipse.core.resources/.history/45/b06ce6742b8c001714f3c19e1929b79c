import com.google.maps.errors.ApiException;
import com.google.maps.model.GeocodingResult;

public class testClass {

	public static void main(String[] args) throws ApiException, InterruptedException, IOException {
		
		double[] coord1 = Mapping.getCoordinates("Snider High School");
		double[] coord2 = Mapping.getCoordinates("Bishop Dwenger High School");
		System.out.println("Dwenger" + Mapping.getCoordinates("Bishop Dwenger High School")[0] + " " + Mapping.getCoordinates("Bishop Dwenger High School")[1]);
		System.out.println("Dwenger" + Mapping.getCoordinates("Snider High School")[0] + " " + Mapping.getCoordinates("Snider High School")[1]);
		System.out.println(Mapping.getDistanceBetween(coord1[0], coord1[1], coord2[0], coord2[1]));
	}

}
